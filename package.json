{
  "name": "ciscospark-survey-bot",
  "version": "1.0.0",
  "description": "Bot for taking surveys",
  "main": "lib/index.js",
  "engines": {
    "node": "8.1.4"
  },
  "scripts": {
    "prod": "node lib/index.js",
    "start": "concurrently  'yarn server-dev'  'gulp'  'yarn redis'",
    "server-dev": "./inenv.sh babel-watch src/index.js",
    "ngrok": "ngrok http 3000 -subdomain=sparksurvey",
    "redis": "redis-server",
    "test": "ava",
    "test-watch": "ava --watch",
    "transpile": "babel --out-dir=lib src",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "webui-dummy-server": "babel-watch src/dummy-ui.js",
    "webui-dev": "concurrently  'yarn webui-dummy-server'  'gulp'",
    "build": "gulp build",
    "gulp": "gulp",
    "bower": "bower",
    "postinstall": "echo 'rebuild node-sass b/c Yarn bug'  &&  npm rebuild node-sass  &&  yarn bower install -- --allow-root  &&  yarn gulp clean  &&  yarn gulp build",
    "heroku-postbuild": "yarn gulp clean-transpiled  &&  yarn transpile"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.1.8",
    "botkit": "^0.5.5",
    "botkit-storage-redis": "^1.1.0",
    "bower": "^1.8.0",
    "caminte": "^0.3.6",
    "cookie-session": "^2.0.0-beta.2",
    "express-async-router": "^0.1.12",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-clean": "^0.3.2",
    "gulp-load-plugins": "^1.1.0",
    "gulp-sass": "^2.1.0",
    "jimp": "^0.2.28",
    "json2csv": "^3.11.1",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "passport": "^0.3.2",
    "passport-cisco-spark": "^1.0.3",
    "passport-local": "^1.0.0",
    "passport-oauth": "^1.0.0",
    "phantom-render-stream": "^2.2.0",
    "promisify-node": "^0.4.0",
    "pug": "^2.0.0-beta11",
    "redis-url": "^1.2.1",
    "socket.io": "^2.0.3",
    "stream-to-array": "^2.3.0",
    "string-to-stream": "^1.1.0",
    "tmp": "^0.0.33",
    "url-join": "^2.0.2",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-eslint": "^7.1.1",
    "babel-watch": "^2.0.5",
    "concurrently": "^3.3.0",
    "eslint": "^3.13.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-class-property": "^1.0.6",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "file-type": "^6.1.0",
    "sinon": "^2.3.6"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/testBot.js",
      "!test/setup.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill",
      "./test/setup"
    ],
    "babel": "inherit"
  }
}
